<div class="container">
  <h3>{{model.title}}</h3>
  {{#each model.posts as |post|}}
    <div class="row post-container">
      <div class="post-user col-md-2">
        <p style="font-weight:bold">{{post.username}}</p>
        <p style="font-size:x-small">id:{{post.userId}}</p>
      </div>
      <div class="post-content col-md-10">
        <p>{{post.postContent}}</p>
      </div>
    </div>
  {{/each}}


</div>
